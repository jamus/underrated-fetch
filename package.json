{
  "name": "@jmus/underrated-fetch",
  "version": "1.0.0",
  "description": "Simple caching for outbound requests",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "clean": "del-cli ./lib",
    "tscheck": "tsc --noEmit --project tsconfig.test.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --import tsx --test tests/*.test.ts",
    "prepare": "husky",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "cache",
    "ttl",
    "api",
    "rate-limit",
    "memoize"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamus/underrated-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/jamus/underrated-fetch/issues"
  },
  "homepage": "https://github.com/jamus/underrated-fetch#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.0.0",
    "husky": "^9.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "lint": "^1.1.2"
  }
}
